      "source": [
        "df = pd.DataFrame(columns=['url', 'author', 'title', 'dateline', 'article', 'keyword'])\n",
        "\n",
        "keywords = ['medicine', 'breakthrough', 'upcoming', 'candy', 'firework', 'science', 'party']\n",
        "\n",
        "for eachlink in links:\n",
        "  href = eachlink['href']\n",
        "  if href.startswith('https://news.asu.edu/'):\n",
        "    full_url = href\n",
        "  else:\n",
        "    full_url = 'https://news.asu.edu/' + href\n",
        "\n",
        "  if not df.url.str.contains(full_url).any():\n",
        "    found_keyword = None\n",
        "    for keyword in keywords:\n",
        "      if keyword in full_url:\n",
        "        found_keyword = keyword\n",
        "        break\n",
        "\n",
        "    if found_keyword:\n",
        "      print('getting',full_url)\n",
        "      story = grabarticle(full_url)\n",
        "      if story:\n",
        "        sleep(15)\n",
        "        story['keyword'] = found_keyword\n",
        "        story_df = pd.DataFrame([story])\n",
        "        df = pd.concat([df, story_df], ignore_index=True)\n",
        "\n",
        "display(df.head())"
      ],
